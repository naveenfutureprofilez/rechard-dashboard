									.status-green-dark.tips__status Popular
									.tips__user
										.tips__avatar
											img(src="img/content/avatar.jpg" alt="Avatar")
										.tips__action Time
						a.tips__item(data-fancybox href="https://www.youtube.com/watch?v=_sI_Ps7JSEk" data-caption="Use guidelines")
							.tips__icon
								+i('multiselect')
							.tips__details
								.tips__title Life & work updates
								.tips__line
									.tips__user
										.tips__avatar
											img(src="img/content/avatar-2.jpg" alt="Avatar")
