---
title: "Core - Dashboard Builder"
classSidebarHome: active
pageTitle: "Dashboard"
classHeader: unauthorized
---
extends layouts/layout
block content
	.page__row
		.page__col
			.card
				.card__head
					.title-red.card__title Overview
					select.select.select_small
						option All time
						option In a year
						option Per month
				.overview.js-tabs
					.overview__nav
						a.overview__link.js-tabs-link.active(href="#")
							.overview__icon(style="background-color: #B1E5FC;")
								+i('shopping-bag')
							.overview__details
								.overview__subtitle Customers
									.tooltip(title="Small description")
										+i('info')
								.overview__counter 1024
							.balance.background.negative
								+i('arrow-bottom')
								| 37.8%
						a.overview__link.js-tabs-link(href="#")
							.overview__icon(style="background-color: #CABDFF;")
								+i('activity')
							.overview__details
								.overview__subtitle Income
									.tooltip(title="Small description")
										+i('info')
								.overview__counter 256k
							.balance.background.positive
								+i('arrow-top')
								| 37.8%
					.overview__container
						.overview__item.js-tabs-item(style="display: block;")
							include partials/users
						.overview__item.js-tabs-item
							.overview__chart
								div(id="oveview-income")
			.card
				.card__head
					.title-purple.card__title Product views
					select.select.select_small
						option Last 7 days
						option This month
						option All time
				.card__chart.card__chart_product-views
					div(id="product-views")
			.card
				.card__head
					.title-green.card__title Pro tips
				.tips
					.tips__info Need some ideas for the next product?
					.tips__list
						a.tips__item(data-fancybox href="https://www.youtube.com/watch?v=_sI_Ps7JSEk" data-caption="Use guidelines")
							.tips__icon
								+i('schedule')
							.tips__details
								.tips__title Early access
								.tips__line
									.status-purple.tips__status New
									.tips__user
										.tips__avatar
											img(src="img/content/avatar.jpg" alt="Avatar")
										.tips__action 3 mins read
						a.tips__item(data-fancybox href="https://www.youtube.com/watch?v=_sI_Ps7JSEk" data-caption="Use guidelines")
							.tips__icon
								+i('arrow-right')
							.tips__details
								.tips__title Asset use guidelines
